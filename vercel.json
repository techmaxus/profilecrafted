{
  "version": 2,
  "builds": [
    {
      "src": "profilecrafted-frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "profilecrafted-frontend/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "profilecrafted-frontend/$1"
    }
  ]
}
